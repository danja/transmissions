{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./",
      "paths": {
        "*": ["node_modules/*", "types/*"]
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "**/node_modules/*"],
    "typeAcquisition": {
      "include": ["@rdfjs/types", "grapoi"]
    }
  }