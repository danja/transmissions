{
    "source": {
        "include": [
            "src"
        ],
        "exclude": [
            "src/node_modules"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "verbose": true,
        "recurse": true,
        "destination": "../docs/jsdoc"
    },
    "plugins": [
        "plugins/markdown"
    ]
}