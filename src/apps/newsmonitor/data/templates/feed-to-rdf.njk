<{{ feedURI.uri | safe }}> a <http://rdfs.org/sioc/ns#Forum> ;
    <http://purl.org/dc/elements/1.1/title> "{{ escape(feed.meta.title or url) }}" ;
    {% if feed.meta.description %}<http://purl.org/dc/elements/1.1/description> "{{ escape(feed.meta.description) }}" ;{% endif %}
    {% if feed.meta.link %}<http://rdfs.org/sioc/ns#link> <{{ feed.meta.link }}> ;{% endif %}
    <http://rdfs.org/sioc/ns#feed_url> <{{ url }}> ;
    {% if feed.meta.lastBuildDate %}<http://purl.org/dc/elements/1.1/date> "{{ feed.meta.lastBuildDate }}"^^<http://www.w3.org/2001/XMLSchema#dateTime> ;{% endif %}
    <http://rdfs.org/sioc/ns#num_items> {{ (feed.entries and feed.entries.length) or 0 }} ;
    <http://rdfs.org/sioc/ns#format> "{{ feed.format or '' }}" ;
    <http://purl.org/dc/elements/1.1/created> "{{ now() }}"^^<http://www.w3.org/2001/XMLSchema#dateTime> .
