@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://purl.org/stuff/transmissions/> .

:shiftTarget a :ConfigSet ;
   # :remove "queryResults" ; # dummy
    :rename (:st1) . # consider using blank nodes
    :st1   :pre     "targetDir" ;
            :post    "targetPath"  .

:readConfig a :ConfigSet ;
    :mediaType "text/plain" .

#:containerConfig a :ConfigSet ;
 #   :destination "terrapack.config.json" .

:templateFields a :ConfigSet  ;
   # :remove "content" ; # tidy
    :rename (:f1 :f2 :f3)  .
        # for content
        :f1   :pre    "relPath" ;
              :post "contentBlocks.filename"  .
        :f2   :pre    "content" ;
              :post  "contentBlocks.content"  .
        :f3   :pre    "slug" ;
              :post  "contentBlocks.slug"  .

:accumulator a :ConfigSet ;
    :loglevel "debug" ;
    :sourceField "content" ;
    :targetField "accumulate" ;
    :label "terrapackContent" ;
    :accumulatorType "string" .

:accToContent a :ConfigSet ;
    :rename (:f1)  .
        # for content
        :f1   :pre    "accumulate" ;
              :post "content" .

:fileWriterConfig a :ConfigSet ;
    :contentField "content" ;
    :destinationFile "terrapack-output.txt" .

#:writeConfig a :ConfigSet ;
 #   :loglevel "debug" ;
  #  :contentField "accumulate" ;
   # :destinationFile "terrapack-output.txt" .