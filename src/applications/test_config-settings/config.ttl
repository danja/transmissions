@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix trm: <http://purl.org/stuff/transmission/> .
@prefix t: <http://hyperdata.it/transmissions/> . # for custom terms & instances

t:set1 a trm:ServiceConfig ;
    trm:setValue (t:sv0)  . # consider using blank nodes
    t:sv0   trm:key    "demo" ;
            trm:value    "uses trm:configKey, deprecated"  .

t:set2 a trm:ServiceConfig ;
    trm:setValue (t:setter1)  . # consider using blank nodes
    t:setter1   trm:key    "demo2" ;
            trm:value    "uses trm:"  .