@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix trm: <http://purl.org/stuff/transmission/> .
@prefix t: <http://hyperdata.it/transmissions/> .

t:Conversations a trm:ServiceConfig ;
    trm:key t:conversationsConfig ;
    trm:targetDir "claudish-chat" .

t:jsonFile a trm:Config ;
    trm:key t:sourceFile ;
    trm:value "./src/applications/test_restructure/input/input-01.json" .



t:retree a trm:ServiceConfig ;
    trm:rename (t:pp1 t:pp2 t:pp3) . # consider using blank nodes
    t:pp1   trm:pre     "payload.item.chat_messages" ;
            trm:post    "channel"  .
    t:pp2   trm:pre     "payload.item.uuid" ;
            trm:post    "filename"  . 
    t:pp3   trm:pre     "payload.item.name" ;
            trm:post    "title"  .

